    "@bugsnag/js": "^6.2.0",
    "@bugsnag/plugin-react": "^6.2.0",
    "classnames": "^2.2.6",
    "connect-mongo": "^2.0.3",
    "express": "^4.17.1",
    "express-session": "^1.16.1",
    "file-saver": "^2.0.2",
    "isomorphic-style-loader": "^5.1.0",
    "jszip": "^3.2.1",
    "mersenne-twister": "^1.1.0",
    "mongoose": "^5.5.11",
    "passport": "^0.4.0",
    "passport-auth0": "^1.1.0",
    "passport-twitter": "^1.0.4",
    "react": "^16.8.6",
